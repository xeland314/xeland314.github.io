  descriptionLine1="El mundo se está quedando sin IPv4,"
  descriptionLine2="pero el estándar sigue siendo el rey."
  likeText="Me gusta"
  commentText="Comenta"
  saveText="Guarda"
  tailwindFirstColor="sky"
  tailwindSecondColor="emerald"
  firstColor="#0ea5e9"
  secondColor="#10b981"
/>
